{
  "version": 2,
  "builds": [{ "src": "/api/server.js", "use": "@now/node" }],
  "routes": [
    { "src": "/rhymes", "dest": "/api/server.js" },
    { "src": "/easy-definitions", "dest": "/api/server.js" },
    { "src": "/hard-definitions", "dest": "/api/server.js" },
    { "src": "/definitions-elo", "dest": "/api/server.js" },
    { "src": "/rhymes-elo", "dest": "/api/server.js" },
    { "src": "/", "dest": "/api/server.js" }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "WORDS_API_KEY": "@words_api_key"
  }
}
