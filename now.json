{
  "name": "word-challenges-api",
  "version": 2,
  "builds": [{ "src": "/api/server.js", "use": "@now/node" }],
  "routes": [
    { "src": "/random-rhymes", "dest": "/api/server.js" },
    { "src": "/rhymes", "dest": "/api/server.js" },
    { "src": "/random-definitions", "dest": "/api/server.js" },
    { "src": "/definitions", "dest": "/api/server.js" },
    { "src": "/", "dest": "/api/server.js" }
  ],
  "env": {
    "MONGODB_URI": "@mongodb-uri"
  }
}
